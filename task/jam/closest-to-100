const compare = (num1, num2) => {
    if (((num1 || num2) > 100) || ((num1 || num2) < 1)){
         console.log('You entered an invalid number. Please try again.');
    } else {
        if (num1 > num2){
            console.log(`${num1} is the number closest to 100.`);
        } else {
            console.log(`${num2} is the number closest to 100.`);
        }
    }
}